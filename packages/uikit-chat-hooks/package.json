  "scripts": {
    "test": "jest",
    "build": "bob build",
    "clean": "del lib"
  },
  "keywords": [
    "react",
    "sendbird"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sendbird/sendbird-uikit-react-native.git",
    "directory": "packages/uikit-chat-hooks"
  },
  "author": "Sendbird <support@sendbird.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/sendbird/sendbird-uikit-react-native.git/issues"
  },
  "homepage": "https://github.com/sendbird/sendbird-uikit-react-native.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@sendbird/uikit-utils": "0.1.2"
  },
  "devDependencies": {
    "@types/react": "17.0.2",
    "react": "^16.13.1",
    "react-native-builder-bob": "^0.18.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "sendbird": "^3.1.21"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
