{
  "name": "@sendbird/uikit-react-native",
  "version": "0.1.2",
  "description": "react-native-uikit",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn generate-version",
    "test": "jest",
    "build": "yarn generate-version && bob build",
    "clean": "del lib",
    "generate-version": "node scripts/generate-version.js ./src/version.ts"
  },
  "keywords": [
    "react-native",
    "sendbird"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sendbird/sendbird-uikit-react-native.git",
    "directory": "packages/uikit-react-native"
  },
  "author": "Sendbird <support@sendbird.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/sendbird/sendbird-uikit-react-native.git/issues"
  },
  "homepage": "https://github.com/sendbird/sendbird-uikit-react-native.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@sendbird/uikit-react-native-core": "0.1.2",
    "@sendbird/uikit-react-native-foundation": "0.1.2",
    "@sendbird/uikit-utils": "0.1.2"
  },
  "devDependencies": {
    "@types/react": "17.0.2",
    "@types/react-native": "^0.66.15",
    "date-fns": "^2.28.0",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-builder-bob": "^0.18.0",
    "react-native-safe-area-context": "^3.3.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "date-fns": "^2.28.0",
    "react": ">=16.13.1",
    "react-native": ">=0.63.3",
    "react-native-safe-area-context": "^3.3.2",
    "sendbird": "^3.1.16"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@sendbird/uikit-react-native"
  }
}
